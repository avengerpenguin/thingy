e=\"http://data.linkedmdb.org/resource/film/1795\" />\n    <owl:sameAs rdf:resource=\"http://rdf.freebase.com/ns/m.040k1l\" />\n    <rdfs:label xml:lang=\"zh\">\u5bb6\u6709\u8df3\u72d7</rdfs:label>\n    <rdfs:label xml:lang=\"it\">Il mio cane Skip</rdfs:label>\n    <rdfs:label xml:lang=\"en\">My Dog Skip (film)</rdfs:label>\n    <rdfs:label xml:lang=\"es\">My Dog Skip</rdfs:label>\n    <rdfs:label xml:lang=\"pl\">M\u00f3j przyjaciel, Skip</rdfs:label>\n    <rdfs:label xml:lang=\"ja\">\u30de\u30a4\u30fb\u30c9\u30c3\u30b0\u30fb\u30b9\u30ad\u30c3\u30d7</rdfs:label>\n    <rdfs:label xml:lang=\"pt\">My Dog Skip</rdfs:label>\n    <rdfs:label xml:lang=\"de\">Mein Hund Skip</rdfs:label>\n    <rdfs:label xml:lang=\"nl\">My Dog Skip</rdfs:label>\n    <rdfs:label xml:lang=\"fr\">Mon chien Skip</rdfs:label>\n    <dcterms:subject rdf:resource=\"http://dbpedia.org/resource/Category:Films_based_on_non-fiction_books\" />\n    <dcterms:subject rdf:resource=\"http://dbpedia.org/resource/Category:Films_about_dogs\" />\n    <dcterms:subject rdf:resource=\"http://dbpedia.org/resource/Category:Warner_Bros._films\" />\n    <dcterms:subject rdf:resource=\"http://dbpedia.org/resource/Category:Films_set_in_the_1940s\" />\n    <dcterms:subject rdf:resource=\"http://dbpedia.org/resource/Category:Alcon_Entertainment_films\" />\n    <dcterms:subject rdf:resource=\"http://dbpedia.org/resource/Category:Films_based_on_children&#39;s_books\" />\n    <dcterms:subject rdf:resource=\"http://dbpedia.org/resource/Category:Films_directed_by_Jay_Russell\" />\n    <dcterms:subject rdf:resource=\"http://dbpedia.org/resource/Category:Films_set_in_Mississippi\" />\n    <dcterms:subject rdf:resource=\"http://dbpedia.org/resource/Category:English-language_films\" />\n    <dcterms:subject rdf:resource=\"http://dbpedia.org/resource/Category:2000_films\" />\n    <dcterms:subject rdf:resource=\"http://dbpedia.org/resource/Category:Films_shot_in_Mississippi\" />\n    <dbpprop:hasPhotoCollection rdf:resource=\"http://wifo5-03.informatik.uni-mannheim.de/flickrwrappr/photos/My_Dog_Skip_(film)\" />\n    <foaf:homepage rdf:resource=\"http://mydogskip.warnerbros.com/\" />\n    <dbpedia-owl:wikiPageExternalLink rdf:resource=\"http://www.randomhouse.com/vintage/screen/books/skip.html\" />\n    <dbpedia-owl:wikiPageExternalLink rdf:resource=\"http://mydogskip.warnerbros.com/\" />\n    <dbpprop:budget rdf:datatype=\"http://dbpedia.org/datatype/usDollar\">7000000.0</dbpprop:budget>\n    <dbpprop:caption xml:lang=\"en\">Theatrical release poster</dbpprop:caption>\n    <dbpprop:cinematography xml:lang=\"en\">James L. Carter</dbpprop:cinematography>\n    <dbpprop:country rdf:resource=\"http://dbpedia.org/resource/United_States\" />\n    <dbpprop:director rdf:resource=\"http://dbpedia.org/resource/Jay_Russell\" />\n    <dbpprop:distributor rdf:resource=\"http://dbpedia.org/resource/Warner_Bros.\" />\n    <dbpprop:editing xml:lang=\"en\">Harvey Rosenstock</dbpprop:editing>\n    <dbpprop:editing xml:lang=\"en\">Gary Winter</dbpprop:editing>\n    <dbpprop:gross rdf:datatype=\"http://dbpedia.org/datatype/usDollar\">3.551276E7</dbpprop:gross>\n    <dbpprop:id rdf:datatype=\"http://www.w3.org/2001/XMLSchema#integer\">156812</dbpprop:id>\n    <dbpprop:language rdf:resource=\"http://dbpedia.org/resource/English_language\" />\n    <dbpprop:language rdf:resource=\"http://dbpedia.org/resource/German_language\" />\n    <dbpprop:music rdf:resource=\"http://dbpedia.org/resource/William_Ross_(composer)\" />\n    <dbpprop:name xml:lang=\"en\">My Dog Skip</dbpprop:name>\n    <dbpprop:narrator rdf:resource=\"http://dbpedia.org/resource/Harry_Connick,_Jr.\" />\n    <dbpprop:producer rdf:resource=\"http://dbpedia.org/resource/Mark_Johnson_(producer)\" />\n    <dbpprop:producer rdf:resource=\"http://dbpedia.org/resource/John_Lee_Hancock\" />\n    <dbpprop:producer rdf:resource=\"http://dbpedia.org/resource/Andrew_Kosove\" />\n    <dbpprop:producer rdf:resource=\"http://dbpedia.org/resource/Broderick_Johnson\" />\n    <dbpprop:runtime rdf:datatype=\"http://dbpedia.org/datatype/second\">5700.0</dbpprop:runtime>\n    <dbpprop:starring rdf:resource=\"http://dbpedia.org/resource/Caitlin_Wachs\" />\n    <dbpprop:starring rdf:resource=\"http://dbpedia.org/resource/Kevin_Bacon\" />\n    <dbpprop:starring xml:lang=\"en\">Daylan Honeycutt</dbpprop:starring>\n    <dbpprop:starring rdf:resource=\"http://dbpedia.org/resource/Moose_(dog)\" />\n    <dbpprop:starring rdf:resource=\"http://dbpedia.org/resource/Frankie_Muniz\" />\n    <dbpprop:starring xml:lang=\"en\">Bradley Coryell</dbpprop:starring>\n    <dbpprop:starring rdf:resource=\"http://dbpedia.org/resource/Cody_Linley\" />\n    <dbpprop:starring rdf:resource=\"http://dbpedia.org/resource/Luke_Wilson\" />\n    <dbpprop:starring rdf:resource=\"http://dbpedia.org/resource/Diane_Lane\" />\n    <dbpprop:studio rdf:resource=\"http://dbpedia.org/resource/Alcon_Entertainment\" />\n    <dbpprop:title xml:lang=\"en\">My Dog Skip</dbpprop:title>\n    <dbpprop:writer xml:lang=\"en\">Screenplay:</dbpprop:writer>\n    <dbpprop:writer rdf:resource=\"http://dbpedia.org/resource/Willie_Morris\" />\n    <dbpprop:writer xml:lang=\"en\">Book:</dbpprop:writer>\n    <dbpprop:writer xml:lang=\"en\">Gail Gilchriest</dbpprop:writer>\n    <dbpedia-owl:abstract xml:lang=\"de\">Mein Hund Skip ist ein autobiografisch gef\u00e4rbter US-amerikanischer Kinder- und Jugendfilm des Regisseurs Jay Russell aus dem Jahre 2000. Er basiert auf dem gleichnamigen Roman von Willie Morris.</dbpedia-owl:abstract>\n    <dbpedia-owl:abstract xml:lang=\"it\">Il mio cane Skip \u00e8 un film del 2000 diretto da Jay Russell, con protagonista Frankie Muniz. Il film \u00e8 tratto dal libro autobiografico My Dog Skip di Willie Morris, morto prematuramante durante la fase di completamento del film.</dbpedia-owl:abstract>\n    <dbpedia-owl:abstract xml:lang=\"fr\">Mon chien Skip (My dog Skip) est un film am\u00e9ricain, r\u00e9alis\u00e9 par Jay Russell, sorti sur les \u00e9crans en ao\u00fbt 2000.</dbpedia-owl:abstract>\n    <dbpedia-owl:abstract xml:lang=\"en\">My Dog Skip is a 2000 film, directed by Jay Russell. It is based on the autobiographical book My Dog Skip by Willie Morris. The movie was released March 3, 2000. The movie recounts a few anecdotes about nine-year-old Willie growing up in Yazoo City, Mississippi. The son of a veteran of the Spanish Civil War and a housewife, Willie is the daily victim of three school bullies. Then one day the title character, a dog he names Skip (bought by his mother over his dad&#39;s objections) comes into his life, and everything changes. The dog is Willie&#39;s entry into a world of new and even stronger and closer friendships. Thus, Skip teaches him that the strongest and truest friendships can be just as wonderful and precious like life.</dbpedia-owl:abstract>\n    <dbpedia-owl:abstract xml:lang=\"nl\">My Dog Skip is een film uit 2000 onder regie van Jay Russell. Het is een boekverfilming van een boek van Willie Morris.</dbpedia-owl:abstract>\n    <dbpedia-owl:abstract xml:lang=\"zh\">\u5bb6\u6709\u8df3\u72d7\uff08My Dog Skip\uff09\u662f2000\u5e74\u7684\u4e00\u90e8\u96fb\u5f71\u3002\u7531Jay Russell\u5c0e\u6f14\u3002\u96fb\u5f71\u6539\u7de8\u81ea\u5a01\u5229\u00b7\u83ab\u91cc\u65af\u7684\u81ea\u50b3\u300a\u5bb6\u6709\u8df3\u72d7\u300b\uff08My Dog Skip \uff09\u3002\u96fb\u5f71\u65bc2000\u5e741\u670814\u65e5\u9996\u6620\u3002\u96fb\u5f71\u8b1b\u8ff0\u4e869\u6b72\u7537\u5b69\u5a01\u5229\u00b7\u83ab\u91cc\u65af\u7684\u7ae5\u5e74\u3002\u4ed6\u5728\u5bc6\u897f\u897f\u6bd4\u7684\u4e9e\u8339\u9577\u5927\uff0c\u7236\u89aa\u66fe\u53c3\u52a0\u904e\u897f\u73ed\u7259\u5167\u6230\uff0c\u6bcd\u89aa\u5247\u662f\u4e00\u4f4d\u666e\u901a\u7684\u5bb6\u5ead\u4e3b\u5a66\u3002\u5a01\u5229\u56e0\u8eab\u9ad4\u5f31\u5c0f\u800c\u66fe\u5728\u5b78\u6821\u88ab\u4eba\u6b3a\u8ca0\uff0c\u4f46\u6709\u4e00\u5929\uff0c\u4e00\u96bb\u72d7\u9032\u5165\u4e86\u4ed6\u7684\u751f\u6d3b\uff0c\u800c\u4e00\u5207\u90fd\u56e0\u6b64\u800c\u8b8a\u5316\u3002</dbpedia-owl:abstract>\n    <dbpedia-owl:abstract xml:lang=\"ja\">\u300e\u30de\u30a4\u30fb\u30c9\u30c3\u30b0\u30fb\u30b9\u30ad\u30c3\u30d7\u300f\uff08My Dog Skip\uff09\u306f\u30012000\u5e74\u306e\u30a2\u30e1\u30ea\u30ab\u5408\u8846\u56fd\u306e\u6620\u753b\u3002\u539f\u4f5c\u306f\u30df\u30b7\u30b7\u30c3\u30d4\u30fc\u5dde\u51fa\u8eab\u306e\u4f5c\u5bb6\u3001\u30a6\u30a3\u30ea\u30fc\u30fb\u30e2\u30ea\u30b9\u306e\u300eMy Dog Skip\u300f\u3067\u3001\u81ea\u8eab\u304c\u5c11\u5e74\u671f\u306b\u4f53\u9a13\u3057\u305f\u611b\u72ac\u30b9\u30ad\u30c3\u30d7\u3068\u306e\u601d\u3044\u51fa\u304c\u7db4\u3089\u308c\u305f\u81ea\u4f1d\u5c0f\u8aac\u3067\u3042\u308b\u3002</dbpedia-owl:abstract>\n    <dbpedia-owl:abstract xml:lang=\"es\">My Dog Skip (Mi perro Skip) es una pel\u00edcula del 2000.Est\u00e1 basada en el libro del mismo nombre, escrito por Willie Morris. La pel\u00edcula fue lanzada el 14 de enero de 2000.</dbpedia-owl:abstract>\n    <dbpedia-owl:abstract xml:lang=\"pt\">My Dog Skip (br: Meu Cachorro Skip; pt: Skip, Um Amigo Especial) \u00e9 um filme estadunidense de 2000, dirigido por Jay Russell.O roteiro do filme \u00e9 baseado no livro My Dog Skip, de Willie Morris. O filme estreou em 3 de mar\u00e7o de 2000.Teve um or\u00e7amento total estimado de seis milh\u00f5es de d\u00f3lares e um retorno de bilheteria nos Estados Unidos de 34 milh\u00f5es de d\u00f3lares.</dbpedia-owl:abstract>\n    <dbpedia-owl:abstract xml:lang=\"pl\">M\u00f3j przyjaciel Skip \u2013 film oparty na prawdziwych wydarzeniach, opisanych we wspomnieniach Williego Morrisa, pod tym samym tytu\u0142em. Akcja rozgrywa si\u0119 w Yazoo City, w USA, we wczesnych latach 40. XX wieku. Willie ma 9 lat - jest nie\u015bmia\u0142y, niski i delikatny; stanowi doskona\u0142y cel \u017cart\u00f3w i docink\u00f3w r\u00f3wie\u015bnik\u00f3w. Na 9. urodziny ch\u0142opiec dostaje pieska, kt\u00f3ry staje si\u0119 jego najlepszym przyjacielem i pomaga zdoby\u0107 nowych przyjaci\u00f3\u0142.</dbpedia-owl:abstract>\n    <foaf:name xml:lang=\"en\">My Dog Skip</foaf:name>\n    <dbpedia-owl:budget rdf:datatype=\"http://dbpedia.org/datatype/usDollar\">7000000.0</dbpedia-owl:budget>\n    <dbpedia-owl:country rdf:resource=\"http://dbpedia.org/resource/United_States\" />\n    <dbpedia-owl:director rdf:resource=\"http://dbpedia.org/resource/Jay_Russell\" />\n    <dbpedia-owl:distributor rdf:resource=\"http://dbpedia.org/resource/Warner_Bros.\" />\n    <dbpedia-owl:gross rdf:datatype=\"http://dbpedia.org/datatype/usDollar\">3.551276E7</dbpedia-owl:gross>\n    <dbpedia-owl:language rdf:resource=\"http://dbpedia.org/resource/German_language\" />\n    <dbpedia-owl:language rdf:resource=\"http://dbpedia.org/resource/English_language\" />\n    <dbpedia-owl:musicComposer rdf:resource=\"http://dbpedia.org/resource/William_Ross_(composer)\" />\n    <dbpedia-owl:narrator rdf:resource=\"http://dbpedia.org/resource/Harry_Connick,_Jr.\" />\n    <dbpedia-owl:producer rdf:resource=\"http://dbpedia.org/resource/John_Lee_Hancock\" />\n    <dbpedia-owl:producer rdf:resource=\"http://dbpedia.org/resource/Mark_Johnson_(producer)\" />\n    <dbpedia-owl:producer rdf:resource=\"http://dbpedia.org/resource/Andrew_Kosove\" />\n    <dbpedia-owl:producer rdf:resource=\"http://dbpedia.org/resource/Broderick_Johnson\" />\n    <dbpedia-owl:runtime rdf:datatype=\"http://www.w3.org/2001/XMLSchema#double\">5700.0</dbpedia-owl:runtime>\n    <dbpedia-owl:starring rdf:resource=\"http://dbpedia.org/resource/Caitlin_Wachs\" />\n    <dbpedia-owl:starring rdf:resource=\"http://dbpedia.org/resource/Kevin_Bacon\" />\n    <dbpedia-owl:starring rdf:resource=\"http://dbpedia.org/resource/Bradley_Coryell\" />\n    <dbpedia-owl:starring rdf:resource=\"http://dbpedia.org/resource/Moose_(dog)\" />\n    <dbpedia-owl:starring rdf:resource=\"http://dbpedia.org/resource/Frankie_Muniz\" />\n    <dbpedia-owl:starring rdf:resource=\"http://dbpedia.org/resource/Cody_Linley\" />\n    <dbpedia-owl:starring rdf:resource=\"http://dbpedia.org/resource/Luke_Wilson\" />\n    <dbpedia-owl:starring rdf:resource=\"http://dbpedia.org/resource/Diane_Lane\" />\n    <dbpedia-owl:writer rdf:resource=\"http://dbpedia.org/resource/My_Dog_Skip\" />\n    <dbpedia-owl:writer rdf:resource=\"http://dbpedia.org/resource/Willie_Morris\" />\n    <dbpprop:wordnet_type rdf:resource=\"http://www.w3.org/2006/03/wn/wn20/instances/synset-movie-noun-1\" />\n    <prov:wasDerivedFrom rdf:resource=\"http://en.wikipedia.org/wiki/My_Dog_Skip_(film)?oldid=606726583\" />\n    <rdfs:comment xml:lang=\"it\">Il mio cane Skip \u00e8 un film del 2000 diretto da Jay Russell, con protagonista Frankie Muniz. Il film \u00e8 tratto dal libro autobiografico My Dog Skip di Willie Morris, morto prematuramante durante la fase di completamento del film.</rdfs:comment>\n    <rdfs:comment xml:lang=\"pt\">My Dog Skip (br: Meu Cachorro Skip; pt: Skip, Um Amigo Especial) \u00e9 um filme estadunidense de 2000, dirigido por Jay Russell.O roteiro do filme \u00e9 baseado no livro My Dog Skip, de Willie Morris. O filme estreou em 3 de mar\u00e7o de 2000.Teve um or\u00e7amento total estimado de seis milh\u00f5es de d\u00f3lares e um retorno de bilheteria nos Estados Unidos de 34 milh\u00f5es de d\u00f3lares.</rdfs:comment>\n    <rdfs:comment xml:lang=\"nl\">My Dog Skip is een film uit 2000 onder regie van Jay Russell. Het is een boekverfilming van een boek van Willie Morris.</rdfs:comment>\n    <rdfs:comment xml:lang=\"fr\">Mon chien Skip (My dog Skip) est un film am\u00e9ricain, r\u00e9alis\u00e9 par Jay Russell, sorti sur les \u00e9crans en ao\u00fbt 2000.</rdfs:comment>\n    <rdfs:comment xml:lang=\"zh\">\u5bb6\u6709\u8df3\u72d7\uff08My Dog Skip\uff09\u662f2000\u5e74\u7684\u4e00\u90e8\u96fb\u5f71\u3002\u7531Jay Russell\u5c0e\u6f14\u3002\u96fb\u5f71\u6539\u7de8\u81ea\u5a01\u5229\u00b7\u83ab\u91cc\u65af\u7684\u81ea\u50b3\u300a\u5bb6\u6709\u8df3\u72d7\u300b\uff08My Dog Skip \uff09\u3002\u96fb\u5f71\u65bc2000\u5e741\u670814\u65e5\u9996\u6620\u3002\u96fb\u5f71\u8b1b\u8ff0\u4e869\u6b72\u7537\u5b69\u5a01\u5229\u00b7\u83ab\u91cc\u65af\u7684\u7ae5\u5e74\u3002\u4ed6\u5728\u5bc6\u897f\u897f\u6bd4\u7684\u4e9e\u8339\u9577\u5927\uff0c\u7236\u89aa\u66fe\u53c3\u52a0\u904e\u897f\u73ed\u7259\u5167\u6230\uff0c\u6bcd\u89aa\u5247\u662f\u4e00\u4f4d\u666e\u901a\u7684\u5bb6\u5ead\u4e3b\u5a66\u3002\u5a01\u5229\u56e0\u8eab\u9ad4\u5f31\u5c0f\u800c\u66fe\u5728\u5b78\u6821\u88ab\u4eba\u6b3a\u8ca0\uff0c\u4f46\u6709\u4e00\u5929\uff0c\u4e00\u96bb\u72d7\u9032\u5165\u4e86\u4ed6\u7684\u751f\u6d3b\uff0c\u800c\u4e00\u5207\u90fd\u56e0\u6b64\u800c\u8b8a\u5316\u3002</rdfs:comment>\n    <rdfs:comment xml:lang=\"en\">My Dog Skip is a 2000 film, directed by Jay Russell. It is based on the autobiographical book My Dog Skip by Willie Morris. The movie was released March 3, 2000. The movie recounts a few anecdotes about nine-year-old Willie growing up in Yazoo City, Mississippi. The son of a veteran of the Spanish Civil War and a housewife, Willie is the daily victim of three school bullies.</rdfs:comment>\n    <rdfs:comment xml:lang=\"ja\">\u300e\u30de\u30a4\u30fb\u30c9\u30c3\u30b0\u30fb\u30b9\u30ad\u30c3\u30d7\u300f\uff08My Dog Skip\uff09\u306f\u30012000\u5e74\u306e\u30a2\u30e1\u30ea\u30ab\u5408\u8846\u56fd\u306e\u6620\u753b\u3002\u539f\u4f5c\u306f\u30df\u30b7\u30b7\u30c3\u30d4\u30fc\u5dde\u51fa\u8eab\u306e\u4f5c\u5bb6\u3001\u30a6\u30a3\u30ea\u30fc\u30fb\u30e2\u30ea\u30b9\u306e\u300eMy Dog Skip\u300f\u3067\u3001\u81ea\u8eab\u304c\u5c11\u5e74\u671f\u306b\u4f53\u9a13\u3057\u305f\u611b\u72ac\u30b9\u30ad\u30c3\u30d7\u3068\u306e\u601d\u3044\u51fa\u304c\u7db4\u3089\u308c\u305f\u81ea\u4f1d\u5c0f\u8aac\u3067\u3042\u308b\u3002</rdfs:comment>\n    <rdfs:comment xml:lang=\"es\">My Dog Skip (Mi perro Skip) es una pel\u00edcula del 2000.Est\u00e1 basada en el libro del mismo nombre, escrito por Willie Morris. La pel\u00edcula fue lanzada el 14 de enero de 2000.</rdfs:comment>\n    <rdfs:comment xml:lang=\"de\">Mein Hund Skip ist ein autobiografisch gef\u00e4rbter US-amerikanischer Kinder- und Jugendfilm des Regisseurs Jay Russell aus dem Jahre 2000. Er basiert auf dem gleichnamigen Roman von Willie Morris.</rdfs:comment>\n    <rdfs:comment xml:lang=\"pl\">M\u00f3j przyjaciel Skip \u2013 film oparty na prawdziwych wydarzeniach, opisanych we wspomnieniach Williego Morrisa, pod tym samym tytu\u0142em. Akcja rozgrywa si\u0119 w Yazoo City, w USA, we wczesnych latach 40. XX wieku. Willie ma 9 lat - jest nie\u015bmia\u0142y, niski i delikatny; stanowi doskona\u0142y cel \u017cart\u00f3w i docink\u00f3w r\u00f3wie\u015bnik\u00f3w. Na 9. urodziny ch\u0142opiec dostaje pieska, kt\u00f3ry staje si\u0119 jego najlepszym przyjacielem i pomaga zdoby\u0107 nowych przyjaci\u00f3\u0142.</rdfs:comment>\n    <ns8:runtime rdf:datatype=\"http://dbpedia.org/datatype/minute\">95.0</ns8:runtime>\n    <dbpedia-owl:wikiPageID rdf:datatype=\"http://www.w3.org/2001/XMLSchema#integer\">1038386</dbpedia-owl:wikiPageID>\n    <dbpedia-owl:wikiPageRevisionID rdf:datatype=\"http://www.w3.org/2001/XMLSchema#integer\">606726583</dbpedia-owl:wikiPageRevisionID>\n    <foaf:isPrimaryTopicOf rdf:resource=\"http://en.wikipedia.org/wiki/My_Dog_Skip_(film)\" />\n  </rdf:Description>\n  <rdf:Description rdf:about=\"http://dbpedia.org/resource/My_Dog_Skip_(movie)\">\n    <dbpedia-owl:wikiPageRedirects rdf:resource=\"http://dbpedia.org/resource/My_Dog_Skip_(film)\" />\n  </rdf:Description>\n  <rdf:Description rdf:about=\"http://fr.dbpedia.org/resource/Mon_chien_Skip\">\n    <owl:sameAs rdf:resource=\"http://dbpedia.org/resource/My_Dog_Skip_(film)\" />\n  </rdf:Description>\n  <rdf:Description rdf:about=\"http://de.dbpedia.org/resource/Mein_Hund_Skip\">\n    <owl:sameAs rdf:resource=\"http://dbpedia.org/resource/My_Dog_Skip_(film)\" />\n  </rdf:Description>\n  <rdf:Description rdf:about=\"http://wikidata.dbpedia.org/resource/Q1917925\">\n    <owl:sameAs rdf:resource=\"http://dbpedia.org/resource/My_Dog_Skip_(film)\" />\n  </rdf:Description>\n  <rdf:Description rdf:about=\"http://es.dbpedia.org/resource/My_Dog_Skip\">\n    <owl:sameAs rdf:resource=\"http://dbpedia.org/resource/My_Dog_Skip_(film)\" />\n  </rdf:Description>\n  <rdf:Description rdf:about=\"http://it.dbpedia.org/resource/Il_mio_cane_Skip\">\n    <owl:sameAs rdf:resource=\"http://dbpedia.org/resource/My_Dog_Skip_(film)\" />\n  </rdf:Description>\n  <rdf:Description rdf:about=\"http://ja.dbpedia.org/resource/\u30de\u30a4\u30fb\u30c9\u30c3\u30b0\u30fb\u30b9\u30ad\u30c3\u30d7\">\n    <owl:sameAs rdf:resource=\"http://dbpedia.org/resource/My_Dog_Skip_(film)\" />\n  </rdf:Description>\n  <rdf:Description rdf:about=\"http://nl.dbpedia.org/resource/My_Dog_Skip\">\n    <owl:sameAs rdf:resource=\"http://dbpedia.org/resource/My_Dog_Skip_(film)\" />\n  </rdf:Description>\n  <rdf:Description rdf:about=\"http://pl.dbpedia.org/resource/M\u00f3j_przyjaciel,_Skip\">\n    <owl:sameAs rdf:resource=\"http://dbpedia.org/resource/My_Dog_Skip_(film)\" />\n  </rdf:Description>\n  <rdf:Description rdf:about=\"http://pt.dbpedia.org/resource/My_Dog_Skip\">\n    <owl:sameAs rdf:resource=\"http://dbpedia.org/resource/My_Dog_Skip_(film)\" />\n  </rdf:Description>\n  <rdf:Description rdf:about=\"http://en.wikipedia.org/wiki/My_Dog_Skip_(film)\">\n    <foaf:primaryTopic rdf:resource=\"http://dbpedia.org/resource/My_Dog_Skip_(film)\" />\n  </rdf:Description>\n  <rdf:Description rdf:about=\"http://zh.dbpedia.org/resource/\u5bb6\u6709\u8df3\u72d7\">\n    <owl:sameAs rdf:resource=\"http://dbpedia.org/resource/My_Dog_Skip_(film)\" />\n  </rdf:Description>\n</rdf:RDF>",
