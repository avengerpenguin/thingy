    <owl:sameAs rdf:resource=\"http://rdf.freebase.com/ns/m.08v03f\" />\n    <owl:sameAs rdf:resource=\"http://sv.dbpedia.org/resource/The_Air_I_Breathe\" />\n    <owl:sameAs rdf:resource=\"http://de.dbpedia.org/resource/The_Air_I_Breathe_\u2013_Die_Macht_des_Schicksals\" />\n    <owl:sameAs rdf:resource=\"http://ru.dbpedia.org/resource/\u0412\u043e\u0437\u0434\u0443\u0445,_\u043a\u043e\u0442\u043e\u0440\u044b\u043c_\u044f_\u0434\u044b\u0448\u0443\" />\n    <rdfs:label xml:lang=\"it\">The Air I Breathe</rdfs:label>\n    <rdfs:label xml:lang=\"de\">The Air I Breathe \u2013 Die Macht des Schicksals</rdfs:label>\n    <rdfs:label xml:lang=\"ru\">\u0412\u043e\u0437\u0434\u0443\u0445, \u043a\u043e\u0442\u043e\u0440\u044b\u043c \u044f \u0434\u044b\u0448\u0443</rdfs:label>\n    <rdfs:label xml:lang=\"es\">The Air I Breathe</rdfs:label>\n    <rdfs:label xml:lang=\"pt\">The Air I Breathe</rdfs:label>\n    <rdfs:label xml:lang=\"fr\">The Air I Breathe</rdfs:label>\n    <rdfs:label xml:lang=\"nl\">The Air I Breathe</rdfs:label>\n    <rdfs:label xml:lang=\"ja\">\u72fc\u305f\u3061\u306e\u5831\u916c</rdfs:label>\n    <rdfs:label xml:lang=\"en\">The Air I Breathe</rdfs:label>\n    <dcterms:subject rdf:resource=\"http://dbpedia.org/resource/Category:Directorial_debut_films\" />\n    <dcterms:subject rdf:resource=\"http://dbpedia.org/resource/Category:American_crime_drama_films\" />\n    <dcterms:subject rdf:resource=\"http://dbpedia.org/resource/Category:2008_films\" />\n    <dcterms:subject rdf:resource=\"http://dbpedia.org/resource/Category:Estudios_Churubusco_films\" />\n    <dcterms:subject rdf:resource=\"http://dbpedia.org/resource/Category:Nonlinear_narrative_films\" />\n    <dcterms:subject rdf:resource=\"http://dbpedia.org/resource/Category:Path%C3%A9_films\" />\n    <dcterms:subject rdf:resource=\"http://dbpedia.org/resource/Category:Films_directed_by_Jieho_Lee\" />\n    <dcterms:subject rdf:resource=\"http://dbpedia.org/resource/Category:2000s_crime_drama_films\" />\n    <dcterms:subject rdf:resource=\"http://dbpedia.org/resource/Category:Mexican_thriller_films\" />\n    <dcterms:subject rdf:resource=\"http://dbpedia.org/resource/Category:Film_scores_by_Marcelo_Zarvos\" />\n    <dbpprop:hasPhotoCollection rdf:resource=\"http://wifo5-03.informatik.uni-mannheim.de/flickrwrappr/photos/The_Air_I_Breathe\" />\n    <dbpprop:caption xml:lang=\"en\">Theatrical poster</dbpprop:caption>\n    <dbpprop:cinematography xml:lang=\"en\">Walt Lloyd</dbpprop:cinematography>\n    <dbpprop:country xml:lang=\"en\">United States</dbpprop:country>\n    <dbpprop:director rdf:resource=\"http://dbpedia.org/resource/Jieho_Lee\" />\n    <dbpprop:distributor rdf:resource=\"http://dbpedia.org/resource/Path%C3%A9\" />\n    <dbpprop:distributor rdf:resource=\"http://dbpedia.org/resource/ThinkFilm\" />\n    <dbpprop:editor xml:lang=\"en\">Robert Hoffman</dbpprop:editor>\n    <dbpprop:gross rdf:datatype=\"http://dbpedia.org/datatype/usDollar\">25775.0</dbpprop:gross>\n    <dbpprop:language xml:lang=\"en\">English</dbpprop:language>\n    <dbpprop:music rdf:resource=\"http://dbpedia.org/resource/Marcelo_Zarvos\" />\n    <dbpprop:name xml:lang=\"en\">The Air I Breathe</dbpprop:name>\n    <dbpprop:producer rdf:resource=\"http://dbpedia.org/resource/Darlene_Caama%C3%B1o\" />\n    <dbpprop:producer rdf:resource=\"http://dbpedia.org/resource/Paul_Schiff\" />\n    <dbpprop:producer rdf:resource=\"http://dbpedia.org/resource/Emilio_Diez_Barroso\" />\n    <dbpprop:released rdf:datatype=\"http://www.w3.org/2001/XMLSchema#date\">2008-01-25+02:00</dbpprop:released>\n    <dbpprop:released xml:lang=\"en\">United States limited</dbpprop:released>\n    <dbpprop:starring rdf:resource=\"http://dbpedia.org/resource/Julie_Delpy\" />\n    <dbpprop:starring rdf:resource=\"http://dbpedia.org/resource/Brendan_Fraser\" />\n    <dbpprop:starring rdf:resource=\"http://dbpedia.org/resource/Sarah_Michelle_Gellar\" />\n    <dbpprop:starring rdf:resource=\"http://dbpedia.org/resource/Emile_Hirsch\" />\n    <dbpprop:starring rdf:resource=\"http://dbpedia.org/resource/Andy_Garc%C3%ADa\" />\n    <dbpprop:starring rdf:resource=\"http://dbpedia.org/resource/Forest_Whitaker\" />\n    <dbpprop:starring rdf:resource=\"http://dbpedia.org/resource/Kevin_Bacon\" />\n    <dbpprop:writer xml:lang=\"en\">Bob DeRosa</dbpprop:writer>\n    <dbpprop:writer rdf:resource=\"http://dbpedia.org/resource/Jieho_Lee\" />\n    <dbpedia-owl:abstract xml:lang=\"fr\">The Air I Breathe est un film am\u00e9ricano-mexicain r\u00e9alis\u00e9 par Jieho Lee en 2007.</dbpedia-owl:abstract>\n    <dbpedia-owl:abstract xml:lang=\"ja\">\u300e\u72fc\u305f\u3061\u306e\u5831\u916c\u300f\uff08\u304a\u304a\u304b\u307f\u305f\u3061\u306e\u307b\u3046\u3057\u3085\u3046\u3001The Air I Breathe\uff09\u306f\u30012007\u5e74\u306e\u30e1\u30ad\u30b7\u30b3\u30fb\u30a2\u30e1\u30ea\u30ab\u5408\u4f5c\u6620\u753b\u3002\u97d3\u56fd\u7cfb\u30a2\u30e1\u30ea\u30ab\u4eba\u76e3\u7763\u3001\u30b8\u30a7\u30db\u30fb\u30ea\u30fc\u306e\u76e3\u7763\u30c7\u30d3\u30e5\u30fc\u4f5c\u3067\u3042\u308b\u3002\u65e5\u672c\u5287\u5834\u672a\u516c\u958b\u3060\u304c\u30012010\u5e7412\u67083\u65e5\u306bDVD\u304c\u767a\u58f2\u3055\u308c\u305f\u3002\u307e\u305f2011\u5e749\u67083\u65e5\u306bWOWOW\u3067\u653e\u9001\u3055\u308c\u305f\u3002\u4eba\u751f\u306f4\u3064\u306e\u611f\u60c5\u300c\u5e78 (Happiness)\u300d\u300c\u559c (Pleasure)\u300d\u300c\u60b2 (Sorrow)\u300d\u300c\u611b (Love)\u300d\u306b\u5206\u985e\u3055\u308c\u3066\u3044\u308b\u3068\u3044\u3046\u53e4\u4ee3\u4e2d\u56fd\u306e\u8a00\u3044\u4f1d\u3048\u306b\u57fa\u3065\u304f\u30aa\u30e0\u30cb\u30d0\u30b9\u5f62\u5f0f\u306e\u30b5\u30b9\u30da\u30f3\u30b9\u7fa4\u50cf\u5287\u30024\u3064\u306e\u30a8\u30d4\u30bd\u30fc\u30c9\u305d\u308c\u305e\u308c\u306e\u4e3b\u4eba\u516c\u306e\u540d\u524d\uff08\u672c\u540d\uff09\u306f\u660e\u3089\u304b\u306b\u3055\u308c\u306a\u3044\u3002</dbpedia-owl:abstract>\n    <dbpedia-owl:abstract xml:lang=\"nl\">The Air I Breathe is een film uit 2007 onder regie van Jieho Lee.</dbpedia-owl:abstract>\n    <dbpedia-owl:abstract xml:lang=\"pt\">The Air I Breathe (Ligados pelo Crime (t\u00edtulo no Brasil) ou O Ar Que Respiramos (t\u00edtulo em Portugal)) \u00e9 um filme americano de 2007, dirigido por Jieho Lee e estrelado por Kevin Bacon, Julie Delpy, Brendan Fraser, Andy Garcia, Sarah Michelle Gellar, Emile Hirsch e Forest Whitaker. O conceito do filme \u00e9 baseado em um antigo prov\u00e9rbio chin\u00eas, que divide a vida em quatro emo\u00e7\u00f5es principais: Felicidade, Prazer, Dor e Amor. Unidas umas a outras, essas s\u00e3o as emo\u00e7\u00f5es que resumem toda a exist\u00eancia humana.</dbpedia-owl:abstract>\n    <dbpedia-owl:abstract xml:lang=\"ru\">\u00ab\u0412\u043e\u0437\u0434\u0443\u0445, \u043a\u043e\u0442\u043e\u0440\u044b\u043c \u044f \u0434\u044b\u0448\u0443\u00bb (\u0430\u043d\u0433\u043b. The Air I Breathe) \u2014 \u0445\u0443\u0434\u043e\u0436\u0435\u0441\u0442\u0432\u0435\u043d\u043d\u044b\u0439 \u043a\u0438\u043d\u043e\u0444\u0438\u043b\u044c\u043c.</dbpedia-owl:abstract>\n    <dbpedia-owl:abstract xml:lang=\"de\">The Air I Breathe \u2013 Die Macht des Schicksals (Originaltitel: The Air I Breathe) ist ein US-amerikanisches Liebesdrama des Regisseurs Jieho Lee. Der Film basiert auf einem alten chinesischen Sprichwort, das das Leben in die vier Emotionen Liebe, Kummer, Gl\u00fcck und Vergn\u00fcgen teilt.In den Hauptrollen sind Kevin Bacon, Brendan Fraser, Forest Whitaker und Sarah Michelle Gellar zu sehen.</dbpedia-owl:abstract>\n    <dbpedia-owl:abstract xml:lang=\"it\">The Air I Breathe \u00e8 un film del 2007 diretto da Jieho Lee.</dbpedia-owl:abstract>\n    <dbpedia-owl:abstract xml:lang=\"en\">The Air I Breathe is the 2008 directorial film debut of Korean-American filmmaker Jieho Lee, who co-wrote the script with Bob DeRosa. It stars Kevin Bacon, Julie Delpy, Brendan Fraser, Andy Garcia, Sarah Michelle Gellar, Emile Hirsch, and Forest Whitaker. The film was financed by NALA Investments through its production company NALA Films, and was released on January 25, 2008 in the United States.The concept of the film is based on an ancient Chinese proverb that breaks life down into four emotional cornerstones \u2013 Happiness (Whitaker), Pleasure (Fraser), Sorrow (Gellar), and Love (Bacon). The proverb speaks of these emotions, not as isolated fragments of feelings, but as elements that make up the whole of the human existence. Each of the four protagonists is based on one of the four emotions; and like the proverb their paths are inextricably linked to each other, akin to the Fingers (Garcia) of a hand.None of the four main characters&#39; actual names are mentioned in the whole film, although Gellar&#39;s character&#39;s stage name, &quot;Trista&quot;, is mentioned several times.</dbpedia-owl:abstract>\n    <dbpedia-owl:abstract xml:lang=\"es\">The Air I Breathe (con varios t\u00edtulos para su distribuci\u00f3n en castellano Cuatro vidas, Cuatro vidas y un destino, Fragmentos del destino) es una pel\u00edcula de drama estrenada en el a\u00f1o 2007 dirigida por Jieho Lee y protagonizada por Kevin Bacon, Julie Delpy, Brendan Fraser, Andy Garc\u00eda, Sarah Michelle Gellar, Emile Hirsch y Forest Whitaker.</dbpedia-owl:abstract>\n    <foaf:name xml:lang=\"en\">The Air I Breathe</foaf:name>\n    <dbpedia-owl:country rdf:resource=\"http://dbpedia.org/resource/United_States\" />\n    <dbpedia-owl:director rdf:resource=\"http://dbpedia.org/resource/Jieho_Lee\" />\n    <dbpedia-owl:distributor rdf:resource=\"http://dbpedia.org/resource/ThinkFilm\" />\n    <dbpedia-owl:distributor rdf:resource=\"http://dbpedia.org/resource/Path%C3%A9\" />\n    <dbpedia-owl:gross rdf:datatype=\"http://dbpedia.org/datatype/usDollar\">25775.0</dbpedia-owl:gross>\n    <dbpedia-owl:musicComposer rdf:resource=\"http://dbpedia.org/resource/Marcelo_Zarvos\" />\n    <dbpedia-owl:producer rdf:resource=\"http://dbpedia.org/resource/Paul_Schiff\" />\n    <dbpedia-owl:producer rdf:resource=\"http://dbpedia.org/resource/Darlene_Caama%C3%B1o\" />\n    <dbpedia-owl:producer rdf:resource=\"http://dbpedia.org/resource/Emilio_Diez_Barroso\" />\n    <dbpedia-owl:releaseDate rdf:datatype=\"http://www.w3.org/2001/XMLSchema#date\">2008-01-25+02:00</dbpedia-owl:releaseDate>\n    <dbpedia-owl:starring rdf:resource=\"http://dbpedia.org/resource/Julie_Delpy\" />\n    <dbpedia-owl:starring rdf:resource=\"http://dbpedia.org/resource/Brendan_Fraser\" />\n    <dbpedia-owl:starring rdf:resource=\"http://dbpedia.org/resource/Sarah_Michelle_Gellar\" />\n    <dbpedia-owl:starring rdf:resource=\"http://dbpedia.org/resource/Emile_Hirsch\" />\n    <dbpedia-owl:starring rdf:resource=\"http://dbpedia.org/resource/Andy_Garc%C3%ADa\" />\n    <dbpedia-owl:starring rdf:resource=\"http://dbpedia.org/resource/Forest_Whitaker\" />\n    <dbpedia-owl:starring rdf:resource=\"http://dbpedia.org/resource/Kevin_Bacon\" />\n    <dbpedia-owl:writer rdf:resource=\"http://dbpedia.org/resource/Jieho_Lee\" />\n    <dbpprop:wordnet_type rdf:resource=\"http://www.w3.org/2006/03/wn/wn20/instances/synset-movie-noun-1\" />\n    <prov:wasDerivedFrom rdf:resource=\"http://en.wikipedia.org/wiki/The_Air_I_Breathe?oldid=593783669\" />\n    <rdfs:comment xml:lang=\"ja\">\u300e\u72fc\u305f\u3061\u306e\u5831\u916c\u300f\uff08\u304a\u304a\u304b\u307f\u305f\u3061\u306e\u307b\u3046\u3057\u3085\u3046\u3001The Air I Breathe\uff09\u306f\u30012007\u5e74\u306e\u30e1\u30ad\u30b7\u30b3\u30fb\u30a2\u30e1\u30ea\u30ab\u5408\u4f5c\u6620\u753b\u3002\u97d3\u56fd\u7cfb\u30a2\u30e1\u30ea\u30ab\u4eba\u76e3\u7763\u3001\u30b8\u30a7\u30db\u30fb\u30ea\u30fc\u306e\u76e3\u7763\u30c7\u30d3\u30e5\u30fc\u4f5c\u3067\u3042\u308b\u3002\u65e5\u672c\u5287\u5834\u672a\u516c\u958b\u3060\u304c\u30012010\u5e7412\u67083\u65e5\u306bDVD\u304c\u767a\u58f2\u3055\u308c\u305f\u3002\u307e\u305f2011\u5e749\u67083\u65e5\u306bWOWOW\u3067\u653e\u9001\u3055\u308c\u305f\u3002\u4eba\u751f\u306f4\u3064\u306e\u611f\u60c5\u300c\u5e78 (Happiness)\u300d\u300c\u559c (Pleasure)\u300d\u300c\u60b2 (Sorrow)\u300d\u300c\u611b (Love)\u300d\u306b\u5206\u985e\u3055\u308c\u3066\u3044\u308b\u3068\u3044\u3046\u53e4\u4ee3\u4e2d\u56fd\u306e\u8a00\u3044\u4f1d\u3048\u306b\u57fa\u3065\u304f\u30aa\u30e0\u30cb\u30d0\u30b9\u5f62\u5f0f\u306e\u30b5\u30b9\u30da\u30f3\u30b9\u7fa4\u50cf\u5287\u30024\u3064\u306e\u30a8\u30d4\u30bd\u30fc\u30c9\u305d\u308c\u305e\u308c\u306e\u4e3b\u4eba\u516c\u306e\u540d\u524d\uff08\u672c\u540d\uff09\u306f\u660e\u3089\u304b\u306b\u3055\u308c\u306a\u3044\u3002</rdfs:comment>\n    <rdfs:comment xml:lang=\"pt\">The Air I Breathe (Ligados pelo Crime (t\u00edtulo no Brasil) ou O Ar Que Respiramos (t\u00edtulo em Portugal)) \u00e9 um filme americano de 2007, dirigido por Jieho Lee e estrelado por Kevin Bacon, Julie Delpy, Brendan Fraser, Andy Garcia, Sarah Michelle Gellar, Emile Hirsch e Forest Whitaker. O conceito do filme \u00e9 baseado em um antigo prov\u00e9rbio chin\u00eas, que divide a vida em quatro emo\u00e7\u00f5es principais: Felicidade, Prazer, Dor e Amor.</rdfs:comment>\n    <rdfs:comment xml:lang=\"it\">The Air I Breathe \u00e8 un film del 2007 diretto da Jieho Lee.</rdfs:comment>\n    <rdfs:comment xml:lang=\"ru\">\u00ab\u0412\u043e\u0437\u0434\u0443\u0445, \u043a\u043e\u0442\u043e\u0440\u044b\u043c \u044f \u0434\u044b\u0448\u0443\u00bb (\u0430\u043d\u0433\u043b. The Air I Breathe) \u2014 \u0445\u0443\u0434\u043e\u0436\u0435\u0441\u0442\u0432\u0435\u043d\u043d\u044b\u0439 \u043a\u0438\u043d\u043e\u0444\u0438\u043b\u044c\u043c.</rdfs:comment>\n    <rdfs:comment xml:lang=\"de\">The Air I Breathe \u2013 Die Macht des Schicksals (Originaltitel: The Air I Breathe) ist ein US-amerikanisches Liebesdrama des Regisseurs Jieho Lee. Der Film basiert auf einem alten chinesischen Sprichwort, das das Leben in die vier Emotionen Liebe, Kummer, Gl\u00fcck und Vergn\u00fcgen teilt.In den Hauptrollen sind Kevin Bacon, Brendan Fraser, Forest Whitaker und Sarah Michelle Gellar zu sehen.</rdfs:comment>\n    <rdfs:comment xml:lang=\"nl\">The Air I Breathe is een film uit 2007 onder regie van Jieho Lee.</rdfs:comment>\n    <rdfs:comment xml:lang=\"en\">The Air I Breathe is the 2008 directorial film debut of Korean-American filmmaker Jieho Lee, who co-wrote the script with Bob DeRosa. It stars Kevin Bacon, Julie Delpy, Brendan Fraser, Andy Garcia, Sarah Michelle Gellar, Emile Hirsch, and Forest Whitaker.</rdfs:comment>\n    <rdfs:comment xml:lang=\"fr\">The Air I Breathe est un film am\u00e9ricano-mexicain r\u00e9alis\u00e9 par Jieho Lee en 2007.</rdfs:comment>\n    <rdfs:comment xml:lang=\"es\">The Air I Breathe (con varios t\u00edtulos para su distribuci\u00f3n en castellano Cuatro vidas, Cuatro vidas y un destino, Fragmentos del destino) es una pel\u00edcula de drama estrenada en el a\u00f1o 2007 dirigida por Jieho Lee y protagonizada por Kevin Bacon, Julie Delpy, Brendan Fraser, Andy Garc\u00eda, Sarah Michelle Gellar, Emile Hirsch y Forest Whitaker.</rdfs:comment>\n    <dbpedia-owl:wikiPageID rdf:datatype=\"http://www.w3.org/2001/XMLSchema#integer\">3140828</dbpedia-owl:wikiPageID>\n    <dbpedia-owl:wikiPageRevisionID rdf:datatype=\"http://www.w3.org/2001/XMLSchema#integer\">593783669</dbpedia-owl:wikiPageRevisionID>\n    <foaf:isPrimaryTopicOf rdf:resource=\"http://en.wikipedia.org/wiki/The_Air_I_Breathe\" />\n  </rdf:Description>\n  <rdf:Description rdf:about=\"http://fr.dbpedia.org/resource/The_Air_I_Breathe\">\n    <owl:sameAs rdf:resource=\"http://dbpedia.org/resource/The_Air_I_Breathe\" />\n  </rdf:Description>\n  <rdf:Description rdf:about=\"http://de.dbpedia.org/resource/The_Air_I_Breathe_\u2013_Die_Macht_des_Schicksals\">\n    <owl:sameAs rdf:resource=\"http://dbpedia.org/resource/The_Air_I_Breathe\" />\n  </rdf:Description>\n  <rdf:Description rdf:about=\"http://wikidata.dbpedia.org/resource/Q370893\">\n    <owl:sameAs rdf:resource=\"http://dbpedia.org/resource/The_Air_I_Breathe\" />\n  </rdf:Description>\n  <rdf:Description rdf:about=\"http://es.dbpedia.org/resource/The_Air_I_Breathe\">\n    <owl:sameAs rdf:resource=\"http://dbpedia.org/resource/The_Air_I_Breathe\" />\n  </rdf:Description>\n  <rdf:Description rdf:about=\"http://it.dbpedia.org/resource/The_Air_I_Breathe\">\n    <owl:sameAs rdf:resource=\"http://dbpedia.org/resource/The_Air_I_Breathe\" />\n  </rdf:Description>\n  <rdf:Description rdf:about=\"http://ja.dbpedia.org/resource/\u72fc\u305f\u3061\u306e\u5831\u916c\">\n    <owl:sameAs rdf:resource=\"http://dbpedia.org/resource/The_Air_I_Breathe\" />\n  </rdf:Description>\n  <rdf:Description rdf:about=\"http://ko.dbpedia.org/resource/\ub0b4\uac00_\uc228\uc26c\ub294_\uacf5\uae30\">\n    <owl:sameAs rdf:resource=\"http://dbpedia.org/resource/The_Air_I_Breathe\" />\n  </rdf:Description>\n  <rdf:Description rdf:about=\"http://nl.dbpedia.org/resource/The_Air_I_Breathe\">\n    <owl:sameAs rdf:resource=\"http://dbpedia.org/resource/The_Air_I_Breathe\" />\n  </rdf:Description>\n  <rdf:Description rdf:about=\"http://pt.dbpedia.org/resource/The_Air_I_Breathe\">\n    <owl:sameAs rdf:resource=\"http://dbpedia.org/resource/The_Air_I_Breathe\" />\n  </rdf:Description>\n  <rdf:Description rdf:about=\"http://en.wikipedia.org/wiki/The_Air_I_Breathe\">\n    <foaf:primaryTopic rdf:resource=\"http://dbpedia.org/resource/The_Air_I_Breathe\" />\n  </rdf:Description>\n  <rdf:Description rdf:about=\"http://ca.dbpedia.org/resource/Quatre_vides\">\n    <owl:sameAs rdf:resource=\"http://dbpedia.org/resource/The_Air_I_Breathe\" />\n  </rdf:Description>\n  <rdf:Description rdf:about=\"http://hu.dbpedia.org/resource/L\u00e9legzet_(film)\">\n    <owl:sameAs rdf:resource=\"http://dbpedia.org/resource/The_Air_I_Breathe\" />\n  </rdf:Description>\n  <rdf:Description rdf:about=\"http://no.dbpedia.org/resource/The_Air_I_Breathe\">\n    <owl:sameAs rdf:resource=\"http://dbpedia.org/resource/The_Air_I_Breathe\" />\n  </rdf:Description>\n  <rdf:Description rdf:about=\"http://ru.dbpedia.org/resource/\u0412\u043e\u0437\u0434\u0443\u0445,_\u043a\u043e\u0442\u043e\u0440\u044b\u043c_\u044f_\u0434\u044b\u04